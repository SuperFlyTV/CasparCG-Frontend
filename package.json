            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.4.2",
        "electron": "^7.1.11",
        "electron-packager": "^14.2.1",
        "electron-reload": "^1.5.0",
        "html-webpack-plugin": "^5.5.0",
        "node-sass": "^4.13.1",
        "sass-loader": "^8.0.2",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.1.3",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
