{
    "name": "casparcg_frontend_application",
    "version": "0.0.1",
    "description": "An application designed to make interacting with the CasparCG server much easier",
    "main": "./dist/server/main.js",
    "scripts": {
        "start": "node ./src/build-scripts/build.js",
        "package": "node ./src/build-scripts/package.js",
        "build-server": "tsc",
        "build-server:watch": "tsc -w",
        "bundle-client": "webpack",
        "start-react": "webpack-dev-server",
        "start-electron": "electron ./dist/server/main.js"
